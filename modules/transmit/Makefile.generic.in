# Get common definitions
include ../../../Makefile.inc

# Where to find install
INSTALL=@INSTALL@

MODULE=`basename \`pwd\``

all:

install-remote:

install:
	${INSTALL} -o @INST_USER@ -g @INST_GROUP@ -m 555 @srcdir@/${MODULE} \
	${DESTDIR}@prefix@/mod/transmit/${MODULE}

clean:

veryclean:
